#include <stdio.h>
#include <stdlib.h>


int factorial(int x)
{

   if(x == 0)
    return 1;
   return x * factorial(x-1);
}
int main()
{

    int number;
    printf("Enter a number\n");
    scanf("%d",&number);
    int fac=factorial(number);

    printf("\n %d ! = %d" ,number,fac);

    return 0;
}
